    key <AB10> { [             minus,             slash                         ] };

    key <AC11> { [         semicolon,             colon                         ] };

    key <LSGT> { [               bar,         brokenbar                         ] };

    key <TLDE> { [       Cyrillic_io,       Cyrillic_IO                         ] };


    key <AD01> { [        numerosign,                 1,            q,        Q ] };
    key <AD02> { [       Cyrillic_yu,       Cyrillic_YU,            w,        W ] };
    key <AD03> { [       Cyrillic_ie,       Cyrillic_IE,            e,        E ] };
