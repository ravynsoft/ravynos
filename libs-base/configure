	*)
		LDFLAGS="$LDFLAGS -Wl,-rpath,${libiconv_libdir} -L${libiconv_libdir}"
		LDIR_FLAGS="$LDIR_FLAGS -Wl,-rpath,${libiconv_libdir} -L${libiconv_libdir}";;
esac

  fi

  old_LIBS="$LIBS"
  LIBS="-liconv $LIBS"
  if test "$cross_compiling" = yes; then :
  found_iconv="$cross_found_iconv_liconv";
    if test "$found_iconv" = "yes"; then

$as_echo "#define HAVE_ICONV 1" >>confdefs.h

      { $as_echo "$as_me:${as_lineno-$LINENO}: result: yes, -liconv (via cross.config)" >&5
$as_echo "yes, -liconv (via cross.config)" >&6; }
    else
      LIBS="$old_LIBS"
    fi

else
  cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */
