	"" \
	{ pr23169b.c pr23169c.c } \
	"pr23169a" \
	"pass.out" \
	"$NOPIE_CFLAGS -O2 -g" \
    ] \
    [list \
	"Run pr23169b" \
	"-pie -Wl,--no-as-needed tmpdir/libpr23169a.so" \
	"" \
	{ pr23169b.c pr23169c.c } \
