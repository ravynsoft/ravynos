    ] \
    [list \
	"No PLT (dynamic 1i)" \
	"$NOPIE_LDFLAGS tmpdir/no-plt-check1b.o tmpdir/no-plt-main1.o \
	 tmpdir/no-plt-extern1b.o tmpdir/libno-plt-1a.so" \
