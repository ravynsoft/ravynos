#, c-format
msgid "(default: yes)\n"
msgstr "(défaut: yes)\n"

#: as.c:307 as.c:318 config/tc-i386.c:14835 config/tc-i386.c:14855
#, c-format
msgid "(default: no)\n"
msgstr "(défaut: no)\n"

