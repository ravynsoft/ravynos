msgid ""
"  -f FILE, --file=FILE, --makefile=FILE\n"
"                              Read FILE as a makefile.\n"
msgstr ""
"  -f ФАЙЛ, --file=ФАЙЛ, --makefile=ФАЙЛ\n"
"                              ползване на този ФАЙЛ да управлява "
"изграждането\n"
"                              (makefile)\n"

