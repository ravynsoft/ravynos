msgid ""
"  -E STRING, --eval=STRING    Evaluate STRING as a makefile statement.\n"
msgstr ""
"  --eval=НИЗ, --eval=НИЗ      изчисляване на НИЗа като израз във файловете\n"
"                              управляващи изграждането (makefile)\n"

