#! /bin/sh
. "${srcdir=.}/init.sh"; path_prepend_ . ../src

# Test --repeated option.

: ${MSGUNIQ-msguniq}
${MSGUNIQ} -w 1000 -d -o msguniq-3.tmp "$wabs_srcdir"/msguniq-a.in || Exit 1
LC_ALL=C tr -d '\r' < msguniq-3.tmp > msguniq-3.out || Exit 1

