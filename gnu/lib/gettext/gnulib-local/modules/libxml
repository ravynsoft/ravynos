configure.ac:
gl_LIBXML

Makefile.am:
if INCLUDED_LIBXML
lib_LIBADD += libxml_rpl.la
lib_DEPENDENCIES += libxml_rpl.la
noinst_LTLIBRARIES += libxml_rpl.la
endif

libxml_rpl_la_SOURCES = \
  libxml/DOCBparser.c \
  libxml/HTMLparser.c \
  libxml/HTMLtree.c \
  libxml/SAX.c \
  libxml/SAX2.c \
  libxml/buf.h \
