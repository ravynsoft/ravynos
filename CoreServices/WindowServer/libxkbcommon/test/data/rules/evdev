  lv5:ralt_switch_lock            =       +level5(ralt_switch_lock)
  lv5:lwin_switch_lock            =       +level5(lwin_switch_lock)
  lv5:rwin_switch_lock            =       +level5(rwin_switch_lock)
  lv5:lsgt_switch_lock_cancel     =       +level5(lsgt_switch_lock_cancel)
